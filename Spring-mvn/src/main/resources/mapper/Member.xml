<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wishwingz.repository.MemberRepository">

	<select id="findAll" resultType="com.wishwingz.model.member.Member">
		SELECT
			 a.member_id AS id
			,a.name	
		FROM
			member a
	</select>

</mapper>
